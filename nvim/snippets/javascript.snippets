snippet for
	for( let ${1:i} = ${2:start}; $1 < ${3:end}; $1${4:++}){
		${5}
	}

snippet for-
	for( let ${1:i} = ${2:start}; $1 < ${3:end}; $1--){
		${4}
	}

snippet func
	function ${1}(${2}){
		${3}
	}

snippet if
	if ( ${1} ) {
		${2}
	} else {
		${3}
	}

snippet elseif
	if ( ${1} ) {
		${2}
	} else if ( ${3} ) {
		${4}
	} else {
		${5}
	}

snippet switch
	switch(${1}) {
		case ${2}:
			${3}
			break;
		default:
	}

snippet p
	console.log("${1}");
